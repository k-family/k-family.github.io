

/* Common style */
.content-wrapper figure {
	position: relative;
	z-index: 1;
	display: inline-block;
	overflow: hidden;
	margin: -0.135em;
	width: 100%;
    min-height: 300px;
	background: #051722;
	text-align: center;
	cursor: pointer;
    margin-bottom: 30px;
}

.content-wrapper .col-md-6 figure{
    height: 400px;
}

.content-wrapper .col-md-4 figure{
    height: 250px;
}

.content-wrapper .col-md-3 figure{
    height: 200px;
}

.content-wrapper figure img {
	position: relative;
	display: block;
	min-height: 100%;
	opacity: 0.8;
}

.content-wrapper figure figcaption {
	padding: 2em;
	color: #fff;
	text-transform: uppercase;
	font-size: 1.25em;
	-webkit-backface-visibility: hidden;
	backface-visibility: hidden;
}

.content-wrapper figure figcaption::before,
.content-wrapper figure figcaption::after {
	pointer-events: none;
}

.content-wrapper figure figcaption,
.content-wrapper figure a {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

/* Anchor will cover the whole item by default */
/* For some effects it will show as a button */
.content-wrapper figure a {
	z-index: 1000;
	text-indent: 200%;
	white-space: nowrap;
	font-size: 0;
	opacity: 0;
}

.content-wrapper figure h2 {
	word-spacing: -0.15em;
	font-weight: 300;
}

.content-wrapper figure h2 span {
	font-weight: 800;
}

.content-wrapper figure h2,
.content-wrapper figure p {
	margin: 0;
}

.content-wrapper figure p {
	letter-spacing: 1px;
	font-size: 68.5%;
}

/* Individual effects */
 

/*---------------*/
/***** b3 *****/
/*---------------*/

figure.effect-b3 {
	background: #9e5406;
}

figure.effect-b3 img {
	opacity: 0.7;
	-webkit-transition: opacity 0.35s;
	transition: opacity 0.35s;
}

figure.effect-b3:hover img {
	opacity: 0.4;
}

figure.effect-b3 figcaption::before,
figure.effect-b3 figcaption::after {
	position: absolute;
	top: 30px;
	right: 30px;
	bottom: 30px;
	left: 30px;
	content: '';
	opacity: 0;
	-webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
	transition: opacity 0.35s, transform 0.35s;
}

figure.effect-b3 figcaption::before {
	border-top: 1px solid #fff;
	border-bottom: 1px solid #fff;
	-webkit-transform: scale(0,1);
	transform: scale(0,1);
}

figure.effect-b3 figcaption::after {
	border-right: 1px solid #fff;
	border-left: 1px solid #fff;
	-webkit-transform: scale(1,0);
	transform: scale(1,0);
}

figure.effect-b3 h2 {
	padding-top: 30%;
	-webkit-transition: -webkit-transform 0.35s;
	transition: transform 0.35s;
	-webkit-transform: translate3d(0,-20px,0);
	transform: translate3d(0,-20px,0);
}

figure.effect-b3 p {
	padding: 20px 2.5em;
	opacity: 0;
	-webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
	transition: opacity 0.35s, transform 0.35s;
	-webkit-transform: translate3d(0,20px,0);
	transform: translate3d(0,20px,0);
}

figure.effect-b3:hover figcaption::before,
figure.effect-b3:hover figcaption::after {
	opacity: 1;
	-webkit-transform: scale(1);
	transform: scale(1);
}

figure.effect-b3:hover h2,
figure.effect-b3:hover p {
	opacity: 1;
	-webkit-transform: translate3d(0,0,0);
	transform: translate3d(0,0,0);
}



